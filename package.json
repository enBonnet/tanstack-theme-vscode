{
    "name": "tanstack-theme",
    "version": "1.0.0",
    "displayName": "TanStack Theme",
    "description": "The non Official TanStack Theme for Visual Studio Code",
    "publisher": "enbonnet",
    "license": "MIT",
    "scripts": {
        "build": "node ./scripts/build.js",
        "lint": "node ./scripts/lint.js",
        "package": "vsce package -o ./bin/tanstack.vsix",
        "vscode:prepublish": "npm run build",
        "vsce:publish": "vsce publish"
    },
    "maintainers": [
        "Ender Bonnet"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/enbonnet/tanstack-theme.git"
    },
    "bugs": {
        "url": "https://github.com/enbonnet/tanstack-theme/issues"
    },
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "tanstack",
        "dark",
        "theme",
        "color-theme"
    ],
    "icon": "images/icon.png",
    "contributes": {
        "themes": [
            {
                "label": "TanStack Theme",
                "uiTheme": "vs-dark",
                "path": "./theme/tanstack.json"
            },
            {
                "label": "TanStack Theme Soft",
                "uiTheme": "vs-dark",
                "path": "./theme/tanstack-soft.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.1",
        "prettier": "^3.0.0",
        "tinycolor2": "^1.6.0",
        "vsce": "^2.15.0"
    }
}